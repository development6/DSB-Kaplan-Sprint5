    <script type="text/javascript">

        $(function () {
            LoadChart();
            LoadChartFact();
            $("#ddlPaciente").bind("change", function () {
                LoadChart();
                LoadChartFact();
            });
            $("#rblChartType input").bind("click", function () {
                LoadChart();
                LoadChartFact();
            });
        });
        function LoadChart() {
            var chartType = parseInt($("#rblChartType input:checked").val());
            var inicio = [];
            var final = [];
            var label = [];
            var datachart = {
                labels: label,
                datasets: [
                    {
                        label: "Inicio",
                        backgroundColor: "rgba(90,193,208,0.3)",
                        borderColor: "rgba(151,187,205,0.7)",
                        borderWidth: 1,
                        hoverBackgroundColor: "rgba(151,187,205,1)",
                        hoverBorderColor: "rgba(151,187,205,0.5)",
                        data: inicio
                    },
                    {
                        label: "Final",
                        backgroundColor: "rgba(215,220,67,0.3)",
                        borderColor: "rgba(220,220,220,0.7)",
                        borderWidth: 1,
                        hoverBackgroundColor: "rgba(220,220,220,1)",
                        hoverBorderColor: "rgba(220,220,220,0.5)",
                        data: final
                    }
                ]
            };
            $.ajax({
                type: "POST",
                url: "/Home/AjaxMethod",
                data: "{Paciente: '" + $("#ddlPaciente").val() + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "text",
                success: function (response) {
                    $("#dvChartIndicadores").html("");
                    //$("#dvLegend").html("");
                    //$("#dvTabla").html("");
                    $("#dvMensaje").html("");

                    if (response[9] == ',' || response[0] == ']') { //response[17]==','
                        var div = $("<div />");
                        //div.css("font-size", "18px");
                        div.html("<p class='caja'>Este paciente no posee datos para mostrar</p>");
                        $("#dvMensaje").append(div);
                    };

                    var data = eval(response);
                    //var data = (eval("(" + response.d + ")"));
                    var el = document.createElement('canvas');
                    $("#dvChartIndicadores")[0].appendChild(el);


                    //window.alert(final);
                    //Fix for IE 8
                    if ($.browser.msie && $.browser.version == "8.0") {
                        G_vmlCanvasManager.initElement(el);
                    }

                    for (var i = 0; i < data.length; i++) {
                        label.push(data[i].value0);
                        inicio.push(data[i].value1);
                        final.push(data[i].value2);

                    }
                    

                    //console.log(ingreso);
                    //window.alert(ingreso);

                    var ctx = el.getContext('2d');
                    var userStrengthsChart;

                    switch (chartType) {
                        case 1:
                            userStrengthsChart = new Chart(ctx, {
                                type: 'bar',
                                data: datachart,
                                options: {
                                    title: {
                                        display: true,
                                        text: "Enfermería"
                                    },
                                    annotation: {
                                        events: ['click', 'dblclick', 'mouseover', 'mouseout'],
                                        annotations: [
                                            {
                                                type: 'line',
                                                mode: 'horizontal',
                                                scaleID: 'y-axis-0',
                                                value: 5,
                                                borderColor: 'rgb(255, 99, 132)',
                                                borderWidth: 2,
                                                label: {
                                                    enabled: true,
                                                    content: 'Logrado:' + 5
                                                }

                                            }
                                        ]
                                    },
                                    scales: {
                                        yAxes: [
                                            {
                                                ticks: {
                                                    //fontFamily: "Roboto Mono",
                                                    //fontColor: "#556F7B",
                                                    //fontStyle: "bold",
                                                    beginAtZero: true,
                                                    suggestedMax: 6,
                                                    stepSize: 1
                                                    //maxTicksLimit: 5,
                                                    //padding: 20
                                                },
                                                gridLines: {
                                                    //drawTicks: false,
                                                    //display: false,
                                                    //drawBorder: false
                                                }
                                            }
                                        ],
                                    }
                                }
                            });
                            break;
                        case 2:
                            userStrengthsChart = new Chart(ctx, {
                                type: 'radar',
                                data: datachart,
                                options: {
                                    title: {
                                        display: true,
                                        text: "Enfermería"
                                    },
                                    /*scale: {
                                        ticks: {
                                            beginAtZero: true,
                                        }
                                    }*/
                                }
                            });
                            break;

                    }
                },
                failure: function (response) {
                    alert('Ocurrió un error.');
                },

            });
        }


        function LoadChartFact() {
            $.ajax({
                type: "POST",
                url: "/Home/AjaxMethod1_1",
                data: "{Paciente: '" + $("#ddlPaciente").val() + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "text",
                success: function (response) {
                    $("#dvHTA").html("");
                    $("#dvDM").html("");
                    $("#dvDLP").html("");
                    $("#dvSED").html("");
                    $("#dvSPOB").html("");
                    $("#dvTB").html("");
                    $("#dvOH").html("");
                    $("#dvAF").html("");
                    $("#dvEstres").html("");

                    var temp = [];
                    //window.alert(response);

                    var data = eval(response);
                    for (var i = 0; i < data.length; i++) {
                        temp.push(data[i].text0, data[i].text1, data[i].text2, data[i].text3, data[i].text4, data[i].text5, data[i].text6, data[i].text7, data[i].text8);
                        
                    }
                    $("#dvHTA").append(temp[0]);
                    $("#dvDM").append(temp[1]);
                    $("#dvDLP").append(temp[2]);
                    $("#dvSED").append(temp[3]);
                    $("#dvSPOB").append(temp[4]);
                    $("#dvTB").append(temp[5]);
                    $("#dvOH").append(temp[6]);
                    $("#dvAF").append(temp[7]);
                    $("#dvEstres").append(temp[8]);
                    
                    //window.alert("hola");

                    var el = document.createElement('canvas');
                    $("#dvChart2")[0].appendChild(el);

                    //Fix for IE 8
                    if ($.browser.msie && $.browser.version == "8.0") {
                        G_vmlCanvasManager.initElement(el);
                    }
                    
                },
                failure: function (response) {
                    alert('Ocurrio un error.');
                }
            });
        }


    </script>








